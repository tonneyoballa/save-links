{
  "manifest_version": 3,
  "name": "Save Links",
  "version": "1.0",
  "description": "A Chrome extension to save links.",
  "action": {
    "default_popup": "save.html",
      "default_icon": {
      "16": "icon16.jpg",
      "48": "icon48.jpg",
      "128": "icon128.jpg"
    }
  },
  "permissions": ["storage", "tabs", "identity"],
  "host_permissions": [
    "https://api.airtable.com/*"
  ],
  "icons": {
    "16": "icon16.jpg",
    "48": "icon48.jpg",
    "128": "icon128.jpg"
  },
  "background": {
    "service_worker": "background.js"
  },
  "permissions":["storage","activeTab","scripting"]
}
