{
    "manifest_version": 3,
    "name": "Link Saver",
    "version": "1.0",
    "description": "A Chrome extension to save and organize your favorite links.",
    "action": {
      "default_popup": "save.html"
      }
    ,
    "permissions": [
      "storage",
      "tabs"
    ],
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["script.js"]
      }
    ]
  }
  