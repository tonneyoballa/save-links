{
  "manifest_version": 3,
  "name": "Save Links (sheets)",
  "version": "1.0",
  "description": "Save links to Google Sheets (and n8n).",
  "permissions": [
    "identity",
    "contextMenus",
    "notifications"
  ],
  "host_permissions": [
    "https://docs.google.com/*",
    "http://localhost:5678/*"
  ],
  "background": {
    "service_worker": "bg.js"
  },
  "action": {
    "default_popup": "popup.html"
  }
}
