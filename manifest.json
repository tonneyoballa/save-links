{
    "manifest_version": 3,
    "name": "Save Links",
    "version": "1.0",
    "description": "A Chrome extension to save links.",
    "action": {
      "default_popup": "save.html"
      }
    ,
          "default_icon": {
        "url.jpeg" :"url.jpeg"
      }
    ,
    "permissions": ["storage", "tabs"],
    "icons": {
      "16": "url.jpeg",
      "48": "url.jpeg",
      "128": "url.jpeg"
    }
  }
  