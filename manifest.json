{
  "manifest_version": 3,
  "name": "Save to n8n",
  "version": "1.0",
  "description": "Save links/pages to n8n -> Notion / Google Sheets",
  "permissions": [
    "contextMenus",
    "activeTab",
    "scripting",
    "storage"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "icons": { "24": "icon16.png", "48": "icon48.png", "128": "icon128.png" }
}
