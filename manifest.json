{
  "manifest_version": 3,
  "name": "Save Links",
  "version": "1.0",
  "description": "A Chrome extension to save links.",
  "action": {
    "default_popup": "save.html"
  },
  "permissions": ["storage", "tabs", "identity"],
  "host_permissions": [
    "https://api.airtable.com/*"
  ],
  "icons": {
    "16": "url.jpeg",
    "48": "url.jpeg",
    "128": "url.jpeg"
  },
  "background": {
    "service_worker": "background.js"
  }
}
