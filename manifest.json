{
  "manifest_version": 3,
  "name": "Save to n8n",
  "version": "1.0",
  "description": "Right-click and send a page or link to n8n",
  "permissions": [
    "identity",
    "contextMenus",
    "notifications"
  ],
  "host_permissions": [
    "http://localhost:5678/*"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html"
  }
}
