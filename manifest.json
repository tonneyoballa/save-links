{
  "manifest_version": 3,
  "name": "Save Links",
  "version": "1.0",
  "description": "A Chrome extension to save links.",
  "action": {
    "default_popup": "save.html",
      "default_icon": {
      "24": "icon24.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
  },
  "permissions": ["storage", "tabs", "identity","activeTab","scripting"],
  "host_permissions": [
    "https://api.airtable.com/*"
  ],
  "icons": {
    "24": "icon24.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "background": {
    "service_worker": "background.js"
  }
}
